<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.scutcms.DAO.entity">
    <class name="Position" table="TPOSITION">
        <id name="positionName" column="POSITION_NAME"></id>
        <property name="baseSalary" column="BASE_SALARY"></property>
        <property name="wageHourly" column="WAGE_HOURLY"></property>
        <property name="punishment" column="PUNISHMENT"></property>
    </class>
</hibernate-mapping>